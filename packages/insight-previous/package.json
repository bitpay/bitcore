{
  "name": "@bitpay/insight-previous",
  "description": "A blockchain explorer for Bitcore",
  "version": "8.1.1",
  "repository": "git://github.com/bitpay/bitcore.git",
  "bugs": {
    "url": "https://github.com/bitpay/bitcore/issues"
  },
  "homepage": "https://github.com/bitpay/bitcore",
  "license": "MIT",
  "private": true,
  "keywords": [
    "insight",
    "insight-ui",
    "blockchain",
    "block explorer",
    "bitcoin",
    "bitcore",
    "API"
  ],
  "config": {
    "ionic_sass": "./sass.js"
  },
  "prettier": {
    "singleQuote": true
  },
  "scripts": {
    "clean": "ionic-app-scripts clean",
    "build:prod": "ENV=prod CHAIN=BTC NETWORK=mainnet API_PREFIX=https://api.bitcore.io/api npm run ionic:build --prod",
    "serve:prod": "npm run clean && npm run build:prod && npm run serve",
    "serve": "serve www",
    "ionic:build": "ionic-app-scripts build",
    "ionic:serve": "ionic-app-scripts serve --port 8200",
    "postinstall": "./node_modules/.bin/webdriver-manager update --gecko false",
    "start": "npm run ionic:serve",
    "test": "ng test --watch=false --code-coverage --no-progress",
    "fix:prettier": "prettier '{*,**/*}.{js,ts,md,json,css}' --write",
    "fix:tslint": "tslint --type-check --fix --project ."
  },
  "dependencies": {
    "@angular/common": "4.4.7",
    "@angular/compiler": "4.4.7",
    "@angular/core": "4.4.7",
    "@angular/forms": "4.4.7",
    "@angular/http": "4.4.7",
    "@angular/platform-browser": "4.4.7",
    "@angular/platform-browser-dynamic": "4.4.7",
    "angular2-moment": "1.6.0",
    "angular2-qrcode": "2.0.1",
    "bitcore-lib": "^8.1.1",
    "bitcore-lib-cash": "^8.1.1",
    "ionic-angular": "3.9.2",
    "ionicons": "3.0.0",
    "lodash": "4.17.11",
    "rxjs": "5.5.12",
    "zone.js": "0.8.12"
  },
  "devDependencies": {
    "@angular/cli": "^1.7.4",
    "@angular/compiler-cli": "4.4.7",
    "@ionic/app-scripts": "3.2.1",
    "@types/jasmine": "2.5.41",
    "@types/lodash": "4.14.104",
    "@types/node": "7.0.4",
    "codecov": "2.2.0",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "3.2.0",
    "karma": "1.4.1",
    "karma-chrome-launcher": "2.1.1",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.2.1",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "prettier": "^1.15.3",
    "serve": "^10.1.1",
    "serve-static": "1.12.3",
    "ts-node": "3.0.4",
    "tslint": "5.3.2",
    "tslint-angular": "^1.1.2",
    "tslint-config-prettier": "^1.15.0",
    "tslint-eslint-rules": "4.1.1",
    "typescript": "2.6.2",
    "webdriver-manager": "^12.1.1"
  },
  "engines": {
    "node": ">=8"
  }
}
