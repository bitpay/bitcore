    "lint:prettier": "prettier '{*,**/*}.{js,ts,md,json,css}' --list-different",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier '{*,**/*}.{js,ts,md,json,css}' --write",
    "fix:tslint": "tslint --fix --project .",
    "e2e": "ng e2e"
  },
  "private": true,
