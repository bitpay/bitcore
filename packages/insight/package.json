{
  "name": "@bitpay/insight",
  "version": "8.0.0",
  "description": "A blockchain explorer for Bitcore",
  "homepage": "https://github.com/bitpay/bitcore",
  "repository": "git://github.com/bitpay/bitcore.git",
  "bugs": {
    "url": "https://github.com/bitpay/bitcore/issues"
  },
  "license": "MIT",
  "keywords": [
    "insight",
    "insight-ui",
    "blockchain",
    "block explorer",
    "bitcoin",
    "bitcore",
    "API"
