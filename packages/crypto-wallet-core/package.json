    "@types/node": "10.12.15",
    "@types/web3": "1.0.19",
    "chai": "4.2.0",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "2.8.0",
    "mocha": "6.1.4",
    "ts-node": "8.1.0",
    "tslint": "5.14.0",
    "typescript": "3.3.4000"
  },
